{
  "manifest_version": 3,
  "name": "Auto Scroll",
  "version": "2.0",
  "description": "Automatically scrolls the page.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Start Auto Scroll"
  },
  "background": {
    "service_worker": "background.js"
  }
}
